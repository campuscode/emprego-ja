<h1>Empresas</h1>
<div id="lista_empresas">
	<% @empresas.each do |e| %>
		
		<ul>
			<li><strong>Nome:</strong><%= link_to "#{e.fantasy_name}" %></li>
			<li><strong>Site:</strong><%= e.url %></li>
			<li><strong>Telefone:</strong><%= e.phone %></li>
			<li><strong>Email:</strong><%= e.email %></li>
			<li><strong>CNPJ:</strong><%= e.cnpj %></li>
		</ul>
	<% end %>
</div>